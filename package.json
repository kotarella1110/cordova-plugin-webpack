{
  "name": "cordova-plugin-webpack",
  "version": "0.0.2",
  "description": "Cordova Webpack Plugin",
  "cordova": {
    "id": "cordova-plugin-webpack",
    "platforms": [
      "android",
      "ios",
      "browser"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "build": "rimraf scripts/* && babili src --out-dir scripts",
    "lint": "eslint 'scripts/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kotarella1110/cordova-plugin-webpack.git"
  },
  "keywords": [
    "cordova",
    "webpack",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-browser"
  ],
  "author": "Kotaro Sugawara",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kotarella1110/cordova-plugin-webpack/issues"
  },
  "homepage": "https://github.com/kotarella1110/cordova-plugin-webpack#readme",
  "dependencies": {
    "webpack": "^4.6.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babili": "^0.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4.2.0"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "lint-staged": {
    "scripts/**/*.js": [
      "npm run lint -- --fix",
      "git add"
    ]
  }
}
